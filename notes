CREATE TABLE tbl_city (
    id int NOT NULL AUTO_INCREMENT,
    province varchar(255),
    city varchar(255),
    description varchar(255),
    PRIMARY KEY (id)
);


CREATE TABLE TBL_TASKLISTTAG (
    id int NOT NULL,
    systemId varchar(100),
    modelType varchar(100),
    iipVersion varchar(100),
    taskListStatus varchar(100),
    failed boolean);
    
    
    CREATE TABLE TBL_TASKLISTTAG (
    id int NOT NULL AUTO_INCREMENT,
    province varchar(255),
    city varchar(255),
    description varchar(255),
    PRIMARY KEY (id)
);